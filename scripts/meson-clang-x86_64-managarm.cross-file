
[binaries]
c = 'clang'
cpp = 'clang++'
ar = 'x86_64-managarm-ar'
strip = 'x86_64-managarm-strip'
pkgconfig = 'pkg-config'

[built-in options]
cpp_args = ['-target', 'x86_64-managarm',
	'-gcc-toolchain', '_BUILD_ROOT_/tools/system-gcc']
cpp_link_args = ['-target', 'x86_64-managarm',
	'-gcc-toolchain', '_BUILD_ROOT_/tools/system-gcc']

[host_machine]
system = 'managarm'
cpu_family = 'x86_64'
cpu = 'x86_64'
endian = 'little'

